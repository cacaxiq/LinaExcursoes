@model IEnumerable<LinaExcursoes.Apresentacao.Models.CauroselViewModel>

@{
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomeCarousel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Imagem)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Titulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubTitulo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomeCarousel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Imagem)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubTitulo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { nomeCaurosel = item.NomeCarousel }) |
            @Html.ActionLink("Details", "Details", new { nomeCaurosel = item.NomeCarousel }) |
            @Html.ActionLink("Delete", "Delete", new { nomeCaurosel = item.NomeCarousel })
        </td>
    </tr>
}

</table>
